{
  "restartable": "rs",
  "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "public"
  ],
  "verbose": true,
  "execMap": {
      "": "node",
      "js": "node --harmony"
  },
  "env": {
      "NODE_ENV": "development",
      "PORT": "3000"
  },
  "ext": "js json",
  "legacy-watch": false
}